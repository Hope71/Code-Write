#include<stdio.h>

int main(void)
{
    char d[30]; // 최대 30개의 문자를 저장할 수 있는 저장공간 마련
    int i; // 변수 i 선언
    
    scanf("%s",&d); // 문장 입력
    
    for(i=0;d[i]!='\0';i++) // '\0', 아스키문자의 정수값은 0을 의미하며 NULL 로도 사용할 수 있다.
    {
        printf("\'%c\'\n",d[i]); // 각각의 공간에 저장된 문자들을 0번째 부터 하나씩 출력한다
    }
    
}
